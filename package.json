{
  "name": "trapdog",
  "version": "0.0.1",
  "description": "Safeguard your Express.js and Node.js frameworks with this vigilant security tool, deterring malicious attacks and keeping your applications protected.",
  "main": "index.js",
  "scripts": {
    "build:watch": "npm run build:js -- --watch",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --no-comments",
    "build:types": "tsc --outDir dist",
    "build": "npm run clean && npm run build:js && npm run build:types",
    "clean": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NotReeceHarris/trapdog.git"
  },
  "keywords": [
    "watchdog",
    "honeypot",
    "http",
    "security"
  ],
  "author": "Reece Harris <reeceharris@duck.com>",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/NotReeceHarris/trapdog/issues"
  },
  "homepage": "https://github.com/NotReeceHarris/trapdog#readme",
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-typescript": "^7.23.3",
    "@koa/router": "^12.0.1",
    "@nestjs/common": "^10.3.3",
    "@nestjs/core": "^10.3.3",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-preset-minify": "^0.5.2",
    "express": "^4.18.3",
    "koa": "^2.15.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3"
  }
}
