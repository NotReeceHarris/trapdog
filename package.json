{
  "name": "trapdog",
  "version": "0.0.1",
  "description": "Automatically detects, logs, and tracks potential attacks, empowering you to swiftly block and trace attack vectors for enhanced security of your NodeJS web-app.",
  "main": "index.js",
  "homepage": "https://github.com/NotReeceHarris/trapdog#readme",
  "author": "Reece Harris <reeceharris@duck.com>",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/NotReeceHarris/trapdog/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NotReeceHarris/trapdog.git"
  },
  "dependencies": {
    "semver": "^7.6.0",
    "sqlite3": "^5.1.7",
    "xss-attack-detection": "^1.0.4"
  },
  "scripts": {
    "test": "nodemon tests/express.js",
    "build:watch": "npm run build:js -- --watch",
    "build:js": "babel src/ --out-dir dist --extensions \".ts,.tsx\" --no-comments",
    "build:types": "tsc --outDir dist --resolveJsonModule ",
    "build": "npm run prebuild && npm run build:js && npm run build:types && npm run postbuild",
    "prebuild": "rimraf dist",
    "postbuild": "mv dist/src/* dist/ && rimraf dist/src"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-typescript": "^7.23.3",
    "@koa/router": "^12.0.1",
    "@nestjs/common": "^10.3.3",
    "@nestjs/core": "^10.3.3",
    "@types/node": "^20.11.24",
    "@types/sqlite3": "^3.1.11",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-preset-minify": "^0.5.2",
    "copyfiles": "^2.4.1",
    "express": "^4.18.3",
    "koa": "^2.15.0",
    "koa-html-render": "^1.0.3",
    "nodemon": "^3.1.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "watchdog",
    "honeypot",
    "http",
    "security",
    "monitoring",
    "logging",
    "tracking",
    "detection",
    "attack",
    "vector",
    "nodejs",
    "webapp",
    "express",
    "koa",
    "nestjs"
  ]
}
