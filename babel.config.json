{
    "presets": [
        [
            "@babel/preset-typescript"
        ],
        [
            "minify",
            {
                "deadcode": false // fixes removing of ordered callback arguments
            }
        ],
        [
            "@babel/preset-env",
            {
                "targets": {
                    "node": "current"
                }
            }
        ]
    ],
    "plugins": ["add-module-exports"]
}